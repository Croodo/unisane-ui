{
  "name": "@unisane/kernel",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/client.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.22.0",
    "mongodb": "^7.0.0",
    "ioredis": "^5.3.0",
    "nanoid": "^5.0.0",
    "pino": "^8.16.0",
    "inngest": "^3.0.0",
    "ulid": "^2.3.0",
    "@aws-sdk/client-s3": "^3.400.0",
    "@aws-sdk/s3-request-presigner": "^3.400.0",
    "@upstash/ratelimit": "^1.0.0"
  },
  "devDependencies": {
    "@unisane/typescript-config": "workspace:*",
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
